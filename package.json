{
    "name": "electron-base-material",
    "version": "1.0.0",
    "description": "base code for a material design electron app",
    "main": "background.js",
    "author": "Jordan Moore",
    "license": "MIT",
    "devDependencies": {
        "electron": "^28.2.0",
        "electron-packager": "^15.4.0"
    },
    "scripts": {
        "start": "electron .",
        "package-win": "electron-packager . electron-base-material --overwrite --asar=true --platform=win32 --arch=ia32 --icon=resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=SH --version-string.FileDescription=SH --version-string.ProductName=\"Desmos | Offline Mode\"",
        "package-linux-x64": "electron-packager . electron-base-material --overwrite --asar=true --platform=linux --arch=x64 --icon=resources/icon.png --prune=true --out=release-builds",
        "package-linux-arm64": "electron-packager . electron-base-material --overwrite --asar=true --platform=linux --arch=arm64 --icon=resources/icon.png --prune=true --out=release-builds"
    },
    "dependencies": {}
}
